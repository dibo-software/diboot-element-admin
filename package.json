    "moment": "^2.27.0",
    "normalize.css": "7.0.0",
    "nprogress": "0.2.0",
    "path-to-regexp": "2.4.0",
    "qs": "^6.9.4",
    "screenfull": "4.2.0",
    "v-viewer": "^1.5.1",
